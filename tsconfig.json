{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "declaration": false,
    "noResolve": false,
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "outDir": "./dist/",
    "allowJs": true,
    "lib": [
      "es2015",
      "es2016",
      "es2020",
      "dom"
    ]
  }
}
